# Local Development Environment Variables
# Copy this to .env for local testing

# Supabase Configuration (if you have them)
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# For local development, disable RT proxy to prevent CORS errors
VITE_RT_PROXY_URL=
VITE_OMDB_PROXY_URL=

# Site Configuration for local dev
VITE_SITE_URL=http://localhost:5173

# Google OAuth (you'll need to configure this for localhost)
VITE_GOOGLE_CLIENT_ID=your_google_oauth_client_id

# TMDB API (should work in local dev)
VITE_TMDB_API_KEY=your_tmdb_api_key

# Local development flag
VITE_DEV_MODE=true
